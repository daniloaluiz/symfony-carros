{% extends "::base.html.twig"%}
{% block body %}
<ul>
    {% for produto in produtos %}
        <li>{{ produto.nome }} 
            (peso {{produto.detalhe.peso}}, altura {{produto.detalhe.altura}}, 
            largura: {{produto.detalhe.largura}})
            <ul>
                {% for categoria in produto.categorias %}
                    <li>{{categoria.id}} - {{categoria.nome}}</li>
                 {% endfor %}   
            </ul>
        </li>
    {% endfor %}
</ul>
{% endblock %}
